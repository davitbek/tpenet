{
	"name": "code-area/laraarea-validator",
	"description": "Validations for laravel",
	"keywords": ["laravel", "validator"],
	"license": "MIT",
	"authors": [
		{
			"name": "Codearea",
			"email": "davitzeynalyan@laravel-helper.com",
			"role": "Developer"
		}
	],
	"require": {
		"laravel/framework": "7.*"
	},
	"require-dev": {
		"mockery/mockery": "^1.3.1",
		"phpunit/phpunit": "^8.5"

	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraAreaValidator\\Providers\\AreaValidatorServiceProvider"
			]
		}
	},
	"suggest": {
		"code-area/laraarea-helpers": "Useful functions"
	},
	"autoload": {
		"psr-4": {
			"LaraAreaValidator\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
